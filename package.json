{
  "name": "DateXO",
  "version": "0.0.1",
  "codegenConfig": {
    "name": "DateXO",
    "type": "all",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.datexo"
    }
  },
  "private": true,
  "scripts": {
    "android:dev": "set ENVFILE=.env.development && react-native run-android --mode=devDebug",
    "android:staging": "set ENVFILE=.env.staging && react-native run-android --mode=stagingDebug",
    "android:prod": "set ENVFILE=.env.production && react-native run-android --mode=prodDebug",
    "android:dev:release": "set ENVFILE=.env.development && react-native run-android --mode=devRelease",
    "android:staging:release": "set ENVFILE=.env.staging && react-native run-android --mode=stagingRelease",
    "android:prod:release": "set ENVFILE=.env.production && react-native run-android --mode=prodRelease",
    "ios:dev": "set ENVFILE=.env.development && react-native run-ios --scheme DateXO-Dev",
    "ios:staging": "set ENVFILE=.env.staging && react-native run-ios --scheme DateXO-Staging",
    "ios:prod": "set ENVFILE=.env.production && react-native run-ios --scheme DateXO-Prod"
  },
